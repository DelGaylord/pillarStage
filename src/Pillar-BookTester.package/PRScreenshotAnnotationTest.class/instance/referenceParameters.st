helpers
referenceParameters
	^ OrderedDictionary new
			add: 'packageName' -> 'Pillar-BookTester';
			add: 'className' -> 'PRScreenshotAnnotationTest';
			add: 'methodName' -> 'referenceString';
			add: 'caption' -> 'The referenceString method';
			add: 'width' -> 50;
			add: 'label' -> 'refStr';
			yourself.